        <pitch> 
          <property>/engines/engine/rpm</property>
          <factor>0.000462</factor>
          <min>0.3</min>
          <max>2.0</max>
          <offset>0</offset>
        </pitch>
      </engine-4>
      <engine-0> 
        <name>engine shut down rpm</name>
        <path>Aircraft/spitfireIX/Sounds/merlin_shutdown.wav</path>
        <condition> 
          <property>/engines/engine/running</property>
          <not> 
            <property>controls/engines/engine/cutoff</property>
          </not>
          <less-than-equals> 
            <property>/engines/engine/rpm</property>
            <value>150</value>
          </less-than-equals>
        </condition>
        <volume> 
          <property>/orientation/alpha-deg</property>
          <type>log</type>
          <min>1.0</min>
          <max>1.5</max>
        </volume>
        <volume> 
          <property>/sim/current-view/internal</property>
          <offset>1.0</offset>
          <factor>-0.4</factor>
        </volume>
      </engine-0>
