<?xml version="1.0"?>
  <PropertyList> 
    <path>att-gb.ac</path>

<animation>
    <type>texmultiple</type>
    <object-name>face</object-name>
    <transform>
        <property>orientation/pitch-deg</property>
        <subtype>textranslate</subtype>
        <factor>0.005</factor>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </transform>
    <transform>
        <property>orientation/roll-deg</property>
        <subtype>texrotate</subtype>
        <center>
            <x>0.5</x>
            <y>0.5</y>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </transform>
</animation>
