<?xml version="1.0"?>
  <PropertyList> 
    <path>uc-control.ac</path>
    
<animation>
  <type>rotate</type>
  <object-name>Mesh</object-name>
  <object-name>Mesh.011</object-name>
  <property>controls/gear/gear-down</property>
      <interpolation> 
        <entry>
          <ind>0.0</ind>
          <dep>60</dep>
        </entry>
        <entry>
          <ind>1.0</ind>
          <dep>0.0</dep>
        </entry>
      </interpolation> 
  <center>
   <x-m>-0.03</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>  

<animation>
  <type>pick</type>
  <object-name>Mesh</object-name>
  <object-name>Mesh.011</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>controls/gear/gear-down</property>
    </binding>
  </action>
</animation>
    
    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>/controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>


  </PropertyList>
