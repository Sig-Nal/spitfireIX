<?xml version="1.0"?>

<PropertyList>

 <path>land-light.ac</path>

 <animation>
  <type>select</type>
  <object-name>Light</object-name>
   <condition>
    <equals>
     <property>controls/switches/landing-light</property>
     <value>1</value>
    </equals>
   </condition> 
 </animation>  

 <animation>
  <type>material</type>
  <object-name>Light</object-name>
  <!--object-name>cylinder</object-name-->
   <emission>
   <!--factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop-->
     <red>0.5</red>
     <green>0.5</green>
     <blue>0.8</blue>
			
    <!--offset>0.1</offset-->
   </emission> 
 </animation>  


