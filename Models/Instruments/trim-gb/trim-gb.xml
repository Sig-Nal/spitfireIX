<?xml version="1.0"?>
  <!-- trim indicator -->
  <PropertyList> 
    <path>trim-gb.ac</path>
    <animation> 
      <type>rotate</type>
      <object-name>needle</object-name>
      <property>controls/flight/elevator-trim</property>
      <interpolation> 
        <entry> 
          <ind>-1</ind>
          <dep>-30.5</dep>
        </entry>
        <entry> 
          <ind>0</ind>
          <dep>0</dep>
        </entry>
        <entry> 
          <ind>1</ind>
          <dep>30.5</dep>
        </entry>
      </interpolation>
      <center> 
        <x-m>0.000858668</x-m>
        <y-m>-0.0160007</y-m>
        <z-m>0</z-m>
      </center>
      <axis> 
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
	
	<animation> 
      <type>material</type>
      <emission> 
        <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>

   	<animation> 
      <type>noshadow</type>
    </animation>
 
	<!-- *** LoD *** -->
    <animation> 
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
