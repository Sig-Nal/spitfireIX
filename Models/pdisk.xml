<?xml version="1.0"?>

<PropertyList>
	
	<!--back-->
	<!--right -->
	<!--up -->
	
      	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
  <object-name>noshadow.spdisk</object-name>
	  <object-name>noshadow.fpdisk</object-name>
	</effect>

    <path>pdisk.ac</path>

 <animation>
  <type>select</type>
  <object-name>noshadow.spdisk</object-name>
  <condition>
   <less-than>
    <property>sim/multiplay/generic/float[9]</property>
    <value>2001</value>
   </less-than>

  </condition>
 </animation>

<animation>
  <type>select</type>
  <object-name>noshadow.fpdisk</object-name>
  <condition>
   <greater-than>
    <property>sim/multiplay/generic/float[9]</property>
    <value>2000</value>
   </greater-than>
  </condition>
 </animation>

<animation>
  <type>spin</type>
  <object-name>noshadow.spdisk</object-name>
    <property>sim/multiplay/generic/float[9]</property>
    <factor>0.35</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<animation>
  <type>spin</type>
  <object-name>noshadow.fpdisk</object-name>
    <property>sim/multiplay/generic/float[9]</property>
    <factor>0.01</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>


</PropertyList>
