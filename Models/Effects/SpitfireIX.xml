<?xml version="1.0"?>

<!-- Supermarine Spitfire Mk IX by Detlef Faber -->

<PropertyList>

 <path>spitfire9.ac</path>

  <nasal>
    <load>
      var update = aircraft.overlay_update.new();
      update.add("Aircraft/spitfireIX/Models/AI-Liveries", "sim/model/livery/file");
      update.add("Aircraft/Generic/Logos", "sim/multiplay/generic/string");
